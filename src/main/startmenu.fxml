<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="900.0" prefWidth="1700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.MenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="singleplayer_btn" layoutX="676.0" layoutY="311.0" mnemonicParsing="false" onAction="#startSingleplayerGame" prefHeight="104.0" prefWidth="350.0" text="Singleplayer">
               <font>
                  <Font size="49.0" />
               </font>
            </Button>
            <Button fx:id="hostGame_btn" layoutX="676.0" layoutY="424.0" mnemonicParsing="false" onAction="#leaveGame" prefHeight="104.0" prefWidth="350.0" text="Leave Game">
               <font>
                  <Font size="49.0" />
               </font>
            </Button>
            <Label alignment="CENTER" layoutX="420.0" layoutY="78.0" prefHeight="74.0" prefWidth="861.0" text="Battleship">
               <font>
                  <Font size="96.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
